{
	"$schema": "https://schemas.wp.org/trunk/wp-env.json",
	"core": null,
	"plugins": [
		"./.",
		"https://downloads.wordpress.org/plugin/wpgraphql-ide.latest-stable.zip"
	],
	"themes": [],
	"env": {
		"development": {
			"config": {
				"WP_DEBUG_LOG": "/var/www/html/wp-content/plugins/wp-graphql/tests/_output/debug.log",
				"WP_DEVELOPMENT_MODE": "plugin"
			}
		},
		"tests": {
			"themes": [
				"https://downloads.wordpress.org/theme/twentytwentyone.latest-stable.zip"
			],
			"plugins": [
				"./.",
				"./tests/e2e/plugins/settings-page-spec"
			],
			"config": {
				"FS_METHOD": "direct"
			},
			"mappings": {
				"/wp-content/plugins/wp-graphql": ".",
				"/wp-content/plugins/wp-graphql/.env": ".env.dist"
			}
		}
	},
	"lifecycleScripts": {
		"afterStart": "bash bin/setup-wp-env.sh"
	}
}
