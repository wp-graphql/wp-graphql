{
  "settings": {
    "bullet": "-",
    "listItemIndent": "mixed",
    "fences": true,
    "incrementListMarker": false
  },
  "plugins": [
    "remark-frontmatter",
    "remark-preset-lint-consistent",
    "remark-preset-lint-markdown-style-guide",
    [
      "remark-lint-maximum-line-length",
      false
    ],
    [
      "remark-lint-no-duplicate-headings",
      false
    ]
  ]
}
