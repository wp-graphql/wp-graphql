{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**"]
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": ["build"]
		},
		"test:e2e": {
			"dependsOn": ["build"]
		},
		"test:unit": {},
		"lint": {},
		"lint:php": {},
		"lint:js": {}
	}
}
