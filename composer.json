{
  "name": "wp-graphql/wp-graphql",
  "description": "GraphQL API for WordPress",
  "type": "wordpress-plugin",
  "license": "GPLv3",
  "version": "0.0.22",
  "authors": [
    {
      "name": "Jason Bahl",
      "email": "jasonbahl@mac.com"
    },
    {
      "name": "Edwin Cromley"
    },
    {
      "name": "Ryan Kanner"
    },
    {
      "name": "Hughie Devore"
    }
  ],
  "require": {
    "webonyx/graphql-php": "^0.11.2",
    "ivome/graphql-relay-php": "^0.3.1",
    "wp-cli/wp-cli": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6",
    "satooshi/php-coveralls": "~1.0"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "WPGraphQL\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "bin": [
    "bin/install-nginx",
    "bin/install-wordpress"
  ]
}
